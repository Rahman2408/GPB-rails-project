<ul>
  
    Name: <%= partner.name %><br>
  
    Email: <%= partner.email %><br>
  <% if current_user == partner %>
      <p>(You)</p>
  <% elsif current_user.partners.include?(partner) %>
      <p>(Active Partner) <%= button_to "Remove?", {action: "delete", id: partner.id} %></p>
      
  <% else%>
    <%= button_to "Add Partner", {action: "create", id: partner.id} %>
  <% end%>
</ul>