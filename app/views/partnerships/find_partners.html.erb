<h1>GPB Members</h1>

<%= form_tag user_find_partners_path, method: :get do %>
    <p> 
        <%= label_tag :search, "Search by Name or Email:"  %>
        <%= text_field_tag :query, params[:query]  %>
        <%= submit_tag "Search", name: nil %>
    </p>
<% end%>

<% @partners.each do |p| %>
    <%= render partial: "partners", locals: {partner: p } %><br>
    <% end%>