

<%= form_tag new_user_project_path(user_id: current_user.id), :method => "get" do%>
    <p> 
        <%= label_tag :search, "Search by Name or Email:"  %>
        <%= text_field_tag :query, params[:query]  %>
        <%= submit_tag("Search", :name => nil) %>
    </p>
    <%= collection_check_boxes(:project, :partners, @partners, :id, :name) %>
<% end%>

    
