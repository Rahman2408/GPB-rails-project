<h1>My Projects</h1>

<% @user.projects.each do |p|%>

<h2><%=  link_to p.title, project_path(p) %></h2>

<% end %>

<%= button_to 'New Project', new_project_path, :method => "get" %>

<h1>All Other Projects</h1>

<% Project.all.where_not_mine(@user.id).each do |p|%>

<h2><%=  link_to p.title, project_path(p) %> - by <%= p.owner.name%></h2>

<% end %>